                "schema": {
                  "$ref": "#/components/schemas/elephant.index.DeleteClusterResponse"
                }
              }
            },
            "description": "Method response"
          },
          "default": {
            "description": ""
          }
        },
        "security": [
          {
            "bearer": []
          }
        ],
        "summary": "DeleteCluster",
